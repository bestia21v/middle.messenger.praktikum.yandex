{
  "extends": "airbnb-base",
  "rules": {
    "no-console": "off",
    "import/prefer-default-export": 0,
    "import/extensions": 0
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js", ".ts"]
      }
    }
  },
  "env": {
    "browser": true
  }
}